LUA_VERSION= 5.1

# Installation directories
# System's libraries directory (where binary libraries are installed)
LUA_LIBDIR= "../LuaJIT"

# Lua includes directory
LUA_INC= "../LuaJIT/src"

# Lua library
LUA_LIB= "../LuaJIT/src/libluajit-5.1.dll.a"

# Compilation directives
WARN= -O2
INCS= -I$(LUA_INC)
CFLAGS= -MD $(WARN) $(INCS)
CC= gcc

